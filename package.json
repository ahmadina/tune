    "keywords": [
        "vue",
        "music",
        "player",
        "vuex"
    ],
    "scripts": {
        "api": "export NODE_ENV=development && node_modules/babel-cli/bin/babel-node.js --presets es2015 bin/server",
        "api-prod": "export NODE_ENV=production &&  node_modules/babel-cli/bin/babel-node.js --presets es2015  bin/server",
        "dev": "node webpack-config/dev-server.js",
        "build": "node webpack-config/build.js",
        "unit": "karma start test/unit/karma.conf.js --single-run",
        "e2e": "node test/e2e/runner.js",
        "test": "npm run unit && npm run e2e",
        "update": "node_modules/npm-check-updates/bin/ncu -u && npm install"
    },
