{
    "extends": "airbnb",
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-shadow": 0,
        "consistent-return": 0,
        "no-unused-vars": 0,
        "standard/object-curly-even-spacing": 0,
        "indent": [
            2,
            4
        ],
        "no-param-reassign": 0,
        "arrow-body-style": [
            "error",
            "always"
        ]
    },
    "globals": {
        "bridge": true,
        "chrome": true
    }
}
